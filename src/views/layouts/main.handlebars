  <script>
    function leerCookie() {
      return new Promise((resolve, reject) => {
        fetch('/api/sessions/current')
          .then(response => response.json())
          .then(data => {
            // Mostrar los datos en un alert
            resolve(data)
          })
          .catch(error => {
            reject(error);
          });
      })

    }
  </script>
